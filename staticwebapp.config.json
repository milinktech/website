{
    "platform": {
        "apiRuntime": "dotnet-isolated:8.0"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/_framework/*",
            "/css/*",
            "/*.ico",
            "/*.png",
            "/*.svg"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    }
}